{
  "name": "grok-integration",
  "displayName": "Grok Integration",
  "description": "Integrate Grok AI into VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "grok-integration.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration": {
      "title": "Grok Integration",
      "properties": {
        "grok.apiKey": {
          "type": "string",
          "default": "",
          "description": "Your xAI API key"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  }

